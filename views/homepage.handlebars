<h1 class="display-4 text-center">WatchSocial</h1>
<p class="text-center">Catchphrase about the Website</p>

<ol class="d-flex flex-wrap justify-content-between">
  {{#each movies as |movie|}}
    <div class="movie-card">
  <img
    src="{{poster}}"
    class=""
  ></img>
  <div class="movie-name">{{title}}</div>
  <div class="movie-review"><a href="/movie/{{id}}">Check out reviews</a></div>
</div>
  {{/each}}
</ol>

